{
  "git.enableSmartCommit": true,
  "sort-imports.default-sort-style": "module-scoped",
  "cSpell.userWords": [
    "API's",
    "Kibana",
    "Vieweset",
    "clsx",
    "debouncing",
    "dotfile",
    "dotfiles",
    "geofence",
    "geofences",
    "getenv",
    "ints",
    "jsonify",
    "kubernetes",
    "paginators",
    "permissioned",
    "permissioning",
    "personalizations",
    "pylint",
    "pytz",
    "serializers",
    "shortcode",
    "shortcodes",
    "subheader",
    "superpedestrian",
    "symlink",
    "symlinked",
    "symlinks",
    "testid",
    "toakleaf",
    "twilio",
    "tzinfo",
    "uncompacted",
    "unmount",
    "unmounting"
  ],
  "editor.tabSize": 2,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "gitlens.blame.highlight.locations": ["gutter", "overview", "line"],
  "gitlens.blame.format": "${author|18} ${ago|14}",
  "gitlens.blame.avatars": false,
  "git.autofetch": true,
  "gitlens.recentChanges.highlight.locations": ["line", "overview", "gutter"],
  "gitlens.hovers.currentLine.over": "line",
  "workbench.iconTheme": "gruvbox-material-icon-theme",
  "editor.fontFamily": "Operator Mono Lig",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "window.zoomLevel": 0,
  "python.formatting.autopep8Args": ["--max-line-length", "100", "--aggressive"],
  "python.formatting.provider": "black",
  "workbench.colorTheme": "Gruvbox Dark (Hard)",
  "terminal.integrated.fontFamily": "OperatorMono Nerd Font",
  "terminal.integrated.fontSize": 14,
  "gitlens.codeLens.enabled": false,
  "vim.highlightedyank.enable": true,
  "vim.surround": true,
  "vim.leader": "<space>",
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<Esc>"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    },
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<leader>", "f", "f"],
      "commands": ["workbench.action.quickOpen"]
    },
    {
      "before": ["<C-I>"],
      "commands": ["workbench.action.navigateForward"]
    },
    {
      "before": ["<leader>", "n"],
      "commands": ["workbench.action.splitEditor"]
    },
    {
      "before": ["<leader>", "l"],
      "commands": ["workbench.action.focusNextGroup"]
    },
    {
      "before": ["<leader>", "h"],
      "commands": ["workbench.action.focusPreviousGroup"]
    },
    {
      "before": ["<leader>", "g", "]"],
      "commands": ["editor.action.marker.nextInFiles"]
    },
    {
      "before": ["<leader>", "g", "["],
      "commands": ["editor.action.marker.prevInFiles"]
    },
    {
      "before": ["<leader>", "r", "g"],
      "commands": ["workbench.view.search"]
    },
    {
      "before": ["<leader>", "g", "s"],
      "commands": ["workbench.view.scm"]
    }
  ],
  "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx2G -Xms100m",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "explorer.confirmDelete": false,
  "prettier.printWidth": 100,
  "prettier.requireConfig": true,
  "editor.formatOnSave": true,
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.exclude": {
    "**/.git": false
  }
}
