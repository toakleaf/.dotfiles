[
  {
    "key": "ctrl+left",
    "command": "cursorWordAccessibilityLeft",
    "when": "accessibilityModeEnabled && textInputFocus"
  },
  {
    "key": "alt+left",
    "command": "-cursorWordAccessibilityLeft",
    "when": "accessibilityModeEnabled && textInputFocus"
  },
  {
    "key": "ctrl+right",
    "command": "cursorWordAccessibilityRight",
    "when": "accessibilityModeEnabled && textInputFocus"
  },
  {
    "key": "alt+right",
    "command": "-cursorWordAccessibilityRight",
    "when": "accessibilityModeEnabled && textInputFocus"
  },
  {
    "key": "cmd+h",
    "command": "workbench.action.focusPreviousGroup"
  },
  {
    "key": "cmd+l",
    "command": "workbench.action.focusNextGroup"
  }
]
